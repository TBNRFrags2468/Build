                        break;
                    case 1:
                        $drops[] = drp::get(drp::GOLDEN_SWORD, 0, 1);
                        break;
                    case 2:
                        $drops[] = drp::get(drp::GOLD_NUGGET, 0, 1);
                        break;
                }
            }
        }
        return $drops;

    }
