		return "Tripwire";
	}

	public function getHardness(){
		return 0.1;
	}

	public function canPassThrough(){
		return true;
	}

	protected function recalculateBoundingBox(){
		if($this->getSide(Vector3::SIDE_DOWN) instanceof Transparent){
			return new AxisAlignedBB(
				$this->x,
				$this->y,
				$this->z,
				$this->x + 1,
				$this->y + 0.5,
				$this->z + 1
			);
		}
		else{
			return new AxisAlignedBB(
				$this->x,
				$this->y,
				$this->z,
				$this->x + 1,
				$this->y + 0.09375,
				$this->z + 1
			);
		}
